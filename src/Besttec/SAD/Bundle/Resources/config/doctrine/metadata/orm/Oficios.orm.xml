<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Oficios" table="oficios">
    <id name="numOficio" type="integer" column="num_oficio">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dataCricaoOficio" type="string" column="data_cricao_oficio" length="45" nullable="false"/>
    <field name="descricao" type="text" column="descricao" nullable="false"/>
    <field name="remetente" type="string" column="remetente" length="45" nullable="true"/>
    <field name="destinatario" type="string" column="destinatario" length="45" nullable="true"/>
  </entity>
</doctrine-mapping>
