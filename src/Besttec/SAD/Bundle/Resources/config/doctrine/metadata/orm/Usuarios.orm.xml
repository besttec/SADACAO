<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuarios" table="usuarios">
    <id name="idUsuario" type="integer" column="id_usuario">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomeUsuario" type="string" column="nome_usuario" length="45" nullable="false"/>
    <field name="loginUsuario" type="string" column="login_usuario" length="20" nullable="true"/>
    <field name="senhaUsuario" type="string" column="senha_usuario" length="20" nullable="true"/>
    <field name="cpfUsuario" type="string" column="cpf_usuario" length="11" nullable="true"/>
  </entity>
</doctrine-mapping>
